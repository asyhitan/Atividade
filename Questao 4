public static void insertionSortPorNome(Pessoa[] v, int qtd) {
    for (int i = 1; i < qtd; i++) {
        Pessoa aux = v[i];
        int j = i - 1;

      
        while (j >= 0 && v[j].nome.compareTo(aux.nome) > 1) {
            v[j+1] = v[j];
            j--;
        }

        v[j+1] = aux;
    }
}
